<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wangke.mapper.ChannelMapper">
	<select id="showChannel" resultType="channel">
		SELECT * FROM cms_channel ORDER BY id
	</select>
	<select id="showChannel2" resultType="channel">
		SELECT * FROM cms_channel where id=${value}
	</select>
</mapper>